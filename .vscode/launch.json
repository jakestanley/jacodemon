    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/"
            }
        },
        {
            "name": "Start2",
            "type": "python",
            "request": "launch",
            "program": "start2.py",
            "args": [
                "--playlist",
                "./examples/v3_Season2.csv"
            ]
        },
        {
            "name": "Start2 (Last)",
            "type": "python",
            "request": "launch",
            "program": "start2.py",
            "args": [
                "--playlist",
                "./examples/v3_Season2.csv",
                "--last"
            ]
        },
        {
            "name": "Start2 (No OBS)",
            "type": "python",
            "request": "launch",
            "program": "start2.py",
            "args": [
                "--playlist",
                "./examples/v3_Season2.csv",
                "--no-obs"
            ]
        },
        {
            "name": "Start2 (No OBS, No Demo)",
            "type": "python",
            "request": "launch",
            "program": "start2.py",
            "args": [
                "--playlist",
                "./examples/v3_Season2.csv",
                "--no-obs",
                "--no-demo"
            ]
        },
        {
            "name": "Start2 (No OBS, No Demo, Quiet)",
            "type": "python",
            "request": "launch",
            "program": "start2.py",
            "args": [
                "--playlist",
                "./examples/v3_Season2.csv",
                "--no-obs",
                "--no-demo",
                "--stdout-log-level", "error"
            ]
        },
        {
            "name": "Start2 (No OBS, No Demo, Last)",
            "type": "python",
            "request": "launch",
            "program": "start2.py",
            "args": [
                "--playlist",
                "./examples/v3_Season2.csv",
                "--no-obs",
                "--no-demo",
                "--last"
            ]
        },
        {
            "name": "Start2 (No Auto, No Demo, Last)",
            "type": "python",
            "request": "launch",
            "program": "start2.py",
            "args": [
                "--playlist",
                "./examples/v3_Season2.csv",
                "--no-auto-record",
                "--no-demo",
                "--last"
            ]
        },
        {
            "name": "Start2 (Replay)",
            "type": "python",
            "request": "launch",
            "program": "start2.py",
            "args": [
                "--playlist",
                "./examples/v3_Season2.csv",
                "--replay"
            ]
        },
        {
            "name": "Start2 (Replay + No OBS)",
            "type": "python",
            "request": "launch",
            "program": "start2.py",
            "args": [
                "--playlist",
                "./examples/v3_Season2.csv",
                "--replay", "--no-obs"
            ]
        },
        {
            "name": "Analyse",
            "type": "python",
            "request": "launch",
            "program": "analyse.py",
            "args": [
                "--playlist",
                "./examples/v3_Season2.csv"
            ]
        }
    ]
}