    "version": "0.2.0",
    "configurations": [
        {
            "name": "Start: Debug, NoObs, Jokes",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/start.ps1",
            "args": [
                "-ConfigPath", "./config.json", 
                "-MapListCsv", "./Jokes.csv", 
                "-Debug", 
                "-NoDemo", 
                "-NoObs"
            ]
        },
        {
            "name": "Start: Debug, NoObs, NoDemo, ModList",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/start.ps1",
            "args": [
                "-ConfigPath", "./config.json", 
                "-ModListCsv", "./ModListSeason1.csv",
                "-Debug", 
                "-NoDemo", 
                "-NoObs"
            ]
        },
        {
            "name": "Start (Mac): Debug, NoObs",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/start.ps1",
            "args": ["-ConfigPath", "./config-mac.json", "-Debug", "-NoObs"]
        }, {
            "name": "Start (Mac): Debug, NoObs, NoDemo",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/start.ps1",
            "args": ["-ConfigPath", "./config-mac.json", "-Debug", "-NoObs", "-NoDemo"]
        }, {
            "name": "Start (Mac): Debug, NoObs, ReRecord",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/start.ps1",
            "args": ["-ConfigPath", "./config-mac.json", "-Debug", "-NoObs", "-ReRecord"]
        }, {
            "name": "Start (Mac): Debug, Random, NoObs, NoDemo",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/start.ps1",
            "args": ["-ConfigPath", "./config-mac.json", "-Debug", "-NoObs", "-Random", "-NoDemo"]
        }, {
            "name": "Analyse (Mac): Debug",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/analyse.ps1",
            "args": ["-ConfigPath", "./config-mac.json", "-Debug"]
        }, {
            "name": "Tests: Debug",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/test.ps1",
            "args": ["-Debug"]
        }
    ]
}